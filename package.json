{
  "name": "v2rayx",
  "version": "0.4.6",
  "description": "An all-platform (Macos Windows Linux) V2ray client build with electron.",
  "main": "./out/main/index.js",
  "author": "shaonhuang",
  "homepage": "https://github.com/shaonhuang/V2rayX",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "publish:local": "electron-vite build && electron-builder",
    "publish:win": "npm run build:win && electron-builder --win --publish always",
    "publish:mac": "npm run build:mac && electron-builder --mac --publish always",
    "publish:linux": "npm run build:linux && electron-builder --linux --publish always",
    "publish:all": "npm run build && electron-builder --win --mac --linux --publish always",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/system": "^5.15.20",
    "@mui/x-data-grid": "^7.7.0",
    "@mui/x-date-pickers": "^7.7.0",
    "@sentry/electron": "^5.1.0",
    "@tanstack/react-query": "^5.45.1",
    "adm-zip": "^0.5.14",
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "compare-versions": "^6.1.0",
    "cron": "^3.1.7",
    "desktop-screenshot": "^0.1.1",
    "electron-progressbar": "^2.2.1",
    "electron-updater": "6.2.1",
    "eventemitter3": "^5.0.1",
    "fs-extra": "^11.2.0",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "material-react-table": "^2.13.0",
    "monaco-editor": "^0.49.0",
    "net": "^1.0.2",
    "object-hash": "^3.0.0",
    "qrcode": "^1.5.3",
    "react-router-dom": "^6.23.1",
    "redux-logger": "^3.0.6",
    "rxjs": "^7.8.1",
    "tapable": "^2.2.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.3.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/adm-zip": "^0.5.5",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.5",
    "@types/object-hash": "^3.0.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@wdio/cli": "^8.39.0",
    "@wdio/local-runner": "^8.39.0",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/spec-reporter": "^8.39.0",
    "@wdio/types": "^8.39.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "electron": "^31.0.1",
    "electron-builder": "24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^4.20.0",
    "html-to-text": "^9.0.5",
    "js-base64": "^3.7.7",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "wdio-electron-service": "^6.6.1",
    "webdriverio": "^8.39.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
